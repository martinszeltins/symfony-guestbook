{% extends 'base.html.twig' %}

{% block title %}Hello ConferenceController!{% endblock %}

{% block body %}
    All conferences:

    {% for conference in conferences %}
        <h4>{{ conference }}</h4>

        <p>
            <a href="{{ path('conference', { id: conference.id }) }}">View</a>
        </p>
    {% endfor %}
{% endblock %}
